{
	"manifest_version": 3,
	"name": "TFS Linked",
	"short_name": "TFS Linked",
	"version": "1.0",
	"description": "Link Jira With TFS",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"permissions": [
		"storage"
	],
	"icons": {
		"48": "icons/favicon-48.png",
		"96": "icons/favicon-96.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://epicor.atlassian.net/*"
			],
			"js": [
				"jquery-3.1.1.min.js",
				"options.js",
				"TFSLlinkedscanner.js"
			]
		}
	]
}